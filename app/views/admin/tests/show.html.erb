<div class="container">
<p class="mt-4">
  <strong><%= t('.title') %>:</strong>
  <%= @test.title %>
</p>

<p>
  <strong><%= t('.level') %>:</strong>
  <%= @test.level %>
</p>
<table class="table">
  <tr>
    <th><%= t('.question') %></th>
    <th colspan='3'><%= t('.action') %></th>
  <tr/>

  <%= render @test.questions %>
</table>


<p><%= link_to t('.link.new'), new_admin_test_question_path(@test) %></p>
<p><%= link_to t('.link.edit'), edit_admin_test_path(@test) %></p>
<p><%= link_to t('.link.back'), admin_tests_path, class:"btn btn-primary" %></p>
</div>
